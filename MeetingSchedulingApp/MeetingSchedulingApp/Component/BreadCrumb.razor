@inject IJSRuntime js;
<div class="d-flex justify-content-between align-items-center">
    <div>
        <div>
            <div class="ul">

                @ChildContent
            </div>
        </div>
    </div>
    <div>
        <span class="btn btn-sm btn-primary" @onclick="GoBack"><i class="fa-solid fa-arrow-left me-1"></i>Back</span>
    </div>
</div>
@code {

    [Parameter]
    public RenderFragment ChildContent { get; set; }
    async Task GoBack()
    {
        await js.InvokeVoidAsync("clientJsfunctions.GoBack");
    }
}
